  <title>DApp Samples</title>
</head>

<body>
  <div id="logo" class="container">
    <h1><span class="icon icon-dashboard icon-size"></span><a href="#">DApp<span>Samples</span></a></h1>
  </div>
  <div id="wrapper" class="container">
    <div id="menu" class="container">
      <ul>
        <li class="current_page_item"><a href="#" accesskey="1" title="">Homepage</a></li>
        <li><a href="#" accesskey="1" title="">ERC20 Token Contract</a></li>
        <li><a href="#" accesskey="2" title="">Read Contract</a></li>
        <li><a href="#" accesskey="3" title="">Write Contract</a></li>
        <li><a href="#" accesskey="4" title="">About Us</a></li>
        <li><a href="#" accesskey="5" title="">Contact Us</a></li>
      </ul>
    </div>
    <div id="three-column" class="container">
      <div id="tbox1">
        <div class="title">
          <h2>Purchase A</h2>
        </div>
        <p>Buy 100 Tokens</p>
        <p id="statusa"></p>
        <a href="#" class="button seta">Buy Tokens</a>
      </div>
      <div id="tbox2">
        <div class="title">
          <h2>Purchase B</h2>
        </div>
        <p>Buy 500 Tokens</p>
        <a href="#" class="button setb">Buy Tokens</a>
      </div>
      <div id="tbox3">
        <div class="title">
          <h2>Countdown Timer</h2>
        </div>
        <p id="sync">Sync with SC timestamp</p>
        <a class="button" id="timer"></a>
      </div>
    </div>

    <div id="page">
      <div id="box1">
        <div class="title">
          <h2>Function 1</h2>
          <p class="byline">Check Balance and Donate</p>
          <p class="byline" id="account"></p>
          <p class="byline" id="donate"></p>
          <button class="button" onclick="getBalance();">Balance</button>
          <button class="button" onclick="donateAccount();">Donate</button>
        </div>
      </div>
      <div id="box2">
        <div class="title">
          <h2>Function 2</h2>
          <p class="byline">Compile and Deploy Contract</p>
          <p class="byline" id="compile"></p>
          <p class="byline" id="deploy"></p>
          <button class="button" onclick="cdContract();">Compile+Deploy</button>
          <button class="button" onclick="deployContract();">Deploy</button>
        </div>
      </div>
    </div>

  </div>
